<serviceconfig name="sap_bapis">
	<apis>
		  <api name="orderdetails"> 
		  		<datasource>jco_sap</datasource>
				<endpoint>orderdetails</endpoint>
						<call><![CDATA[ZBAPI_ORDER_DETAILS]]>
						<parameters>							
						</parameters>
						<cache>
							<maxsize></maxsize>
							<frequency>300</frequency>
						</cache>
						</call>
		  </api> 
		  
		  <api name="wavestats"> 
		  		<datasource>jco_sap</datasource>
				<endpoint>wavestats</endpoint>
						<call><![CDATA[ZBAPI_WAVE_ORDER_PACK_COUNT]]>
						<parameters>
							<parameter>
								<name>i_vdatu</name>
								<type>DATE</type>
								<default>$SYSDATE</default>								
							</parameter>							
						</parameters>
						<cache>
							<maxsize></maxsize>
							<frequency>300</frequency>
						</cache>
						</call>
		  </api> 
		  
		  <api name="shippingstats"> 
				<datasource>jco_sap</datasource>
				<endpoint>shippingstats</endpoint>
						<call><![CDATA[ZBAPI_SHIP_LOAD]]>
						<parameters>
							<parameter>
								<name>delivery_date</name>
								<type>DATE</type>
								<default>$SYSDATE</default>								
							</parameter>
							<parameter>
								<name>route</name>
								<type>STRING</type>
								<default></default>								
							</parameter>
						</parameters>
						<cache>
							<maxsize></maxsize>
							<frequency>300</frequency>
						</cache>
						</call>
		  </api>
		  <api name="shippingdock"> 
				<datasource>jco_sap</datasource>
				<endpoint>shippingdock</endpoint>
						<call><![CDATA[ZBAPI_SHIP_LOAD2]]>
						<parameters>
							<parameter>
								<name>dock_number</name>
								<type>STRING</type>
								<default>01</default>								
							</parameter>
						</parameters>
						<cache>
							<maxsize></maxsize>
							<frequency>300</frequency>
						</cache>
						</call>
		  </api>
		  
	</apis>
</serviceconfig>