<%@ page import="java.net.URLEncoder" %>
<%@ page import="com.freshdirect.common.customer.EnumServiceType" %>
<%@ page import="com.freshdirect.fdstore.FDStoreProperties" %>
<%@page import="com.freshdirect.fdstore.content.MyFD"%>
<table width="745" cellpadding="0" cellspacing="0" border="0">
	<tr>
		<td>
			<img src="/media_stat/images/layout/clear.gif" width="1" height="5" alt="" /><br />
            <% 
                String bottomUri = request.getRequestURI();
                String deptId = request.getParameter("deptId") != null ? request.getParameter("deptId") : "";
                String catId = request.getParameter("catId") != null ? request.getParameter("catId") : "";
                String nomyfd = FDStoreProperties.isMyfdEnabled() && MyFD.getMyFDInstance() != null ? "" : "_nomyfd";
                if (bottomUri.indexOf("/about/index.jsp") > -1) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_aboutus<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if (bottomUri.indexOf("/myfd") > -1) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_myfd<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if (bottomUri.indexOf("/jobs/intro") > -1) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_careers<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if (bottomUri.indexOf("/gift_card/purchase/landing.jsp") > -1) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_giftcards<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if (bottomUri.indexOf("/newproducts.jsp") > -1 || bottomUri.indexOf("/newproducts_dfgs.jsp") > -1) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_newprod<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if(!deptId.equals("") && deptId.equalsIgnoreCase("cmty")) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_community<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if(!catId.equals("") && catId.equalsIgnoreCase("about_billrights")) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_billorrights<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                } else if(!deptId.equals("") && deptId.equalsIgnoreCase("COS")) { 
            %>
            <img src="/media_stat/images/navigation/global_nav/bottom_nav_office<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%            
                } else {
            %>
			<img src="/media_stat/images/navigation/global_nav/bottom_nav_off<%= nomyfd %>.gif" width="746" height="22" alt="" border="0" usemap="#bottomlinks" />
            <%
                }
            %>
	</td>
	</tr>
	<tr>
		<td align="center" class="text11bold">
			<img src="/media_stat/images/layout/clear.gif" width="1" height="14" alt="" />
