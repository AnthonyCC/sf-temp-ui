									<div class="ctaATC">
										<input type="image" class="CTAButtonNFP" id="CTAButtonNFP_<%= seqDDPP %>" name="addSingleToCart_<%= transProdIndex %>" src="/media/images/buttons/add_to_cart_no_icon<%= (cart.getTotalQuantity(pm) > 0) ? "_s": "" %>.png" width="95" height="27" border="0" alt="ADD SELECTED TO CART" />
									</div>
									<div class="prodATCStatus" id="<%= prodATCStatusId %>"></div>
									<% transProdIndex++; %>
								</form>
					<% } else {
							String deptId = pm.getDepartment().getContentKey().getId();
					%>
						<div class="ctaCUSTOMIZE">
							<input type="image" class="CTAButtonNFP" id="CTAButtonNFP_<%= seqDDPP %>" src="/media/images/buttons/customize_blue<%= (cart.getTotalQuantity(pm) > 0) ? "_s": "" %>.png" height="27" width="95" onclick="FD_QuickBuy.showPanel('<%= deptId %>', '<%= pm.getCategory().getContentKey().getId() %>', '<%= pm.getContentKey().getId() %>', '<%= prodATCStatusId %>')();" />
						</div>
						<div class="prodATCStatus" id="<%= prodATCStatusId %>"></div>
