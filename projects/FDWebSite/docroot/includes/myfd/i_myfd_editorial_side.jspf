<%@taglib uri='freshdirect' prefix='fd'%>
<%@page import="java.util.List"%>
<%@page import="com.freshdirect.fdstore.content.MediaModel"%>
<%--@page import="com.freshdirect.fdstore.content.MyFD"--%>
<%-- MyFD myfd = null; --%>
<% List<MediaModel> sideItems = myfd.getEditorialSide(); %>
<% if (sideItems != null && !sideItems.isEmpty()) { %>
<div>
<% for (MediaModel item : sideItems) { %>
<div>
<fd:IncludeMedia name="<%= item.getPath() %>" media="<%= item %>"/>
</div>
<% } %>
</div>
<% } %>