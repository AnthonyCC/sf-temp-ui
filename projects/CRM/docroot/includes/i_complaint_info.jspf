<% if (olit.isCartonized()) { %>
        <td width="2%" align="center"><%= olit.getLastCartonDetail().getCartonDetail().getPackedQuantity() %></td>
<% } else { %>
        <td width="2%" align="center"><%= invoiceLine != null ? invoiceLine.getQuantity() : orderLine.getQuantity() %></td>
<% } %>
