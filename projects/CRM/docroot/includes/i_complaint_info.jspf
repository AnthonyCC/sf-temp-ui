        <td width="4%" bgcolor="<%= bgcolor %>" align="center"><%= orderLine.getSalesUnitDescription() %></td>
        <td width="7%" bgcolor="<%= bgcolor %>" align="center"><%= CCFormatter.formatCurrency( retAmount ) %></td>
        <td width="5%" bgcolor="<%= bgcolor %>" align="right"><%= CCFormatter.formatCurrency( (invoiceLine != null ? invoiceLine.getPrice() : orderLine.getPrice()) - retAmount ) %></td>
