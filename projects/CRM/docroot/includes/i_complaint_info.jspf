		<td colspan="9" style="background-color: <%= bgcolor %>;">&nbsp;</td>
	</tr>
<%
		    }
		    
			if ( (olit.isCartonized() && olit.isCartonNumberChanged()) || olit.isDepartmentNameChanged() ) {	 
