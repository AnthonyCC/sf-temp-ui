<?xml version="1.0"?>
<module id="com.freshdirect.test" version="1.0.0">
	<contribution configuration-id="hivemind.SymbolSources">
		<source name="PropertiesSymbolSource"
			class="org.apache.hivemind.impl.SystemPropertiesSymbolSource"
			before="hivemind.ApplicationDefaults"/>
        </contribution>

	<contribution configuration-id="hivemind.ApplicationDefaults">
