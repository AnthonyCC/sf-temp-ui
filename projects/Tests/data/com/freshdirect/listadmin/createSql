CREATE TABLE SYSTEM_SEQUENCES(SEQUENCE_NAME VARCHAR(50));
CREATE TABLE LIST_VIRTUAL_OBJECT (ID VARCHAR(16) NOT NULL PRIMARY KEY, NAME VARCHAR(30) NOT NULL, SQL_TEXT VARCHAR(2048) NOT NULL, SIMPLE_SQL CHAR(1) DEFAULT 'N');
CREATE TABLE LIST_TEMPLATE (ID VARCHAR(16) NOT NULL PRIMARY KEY, NAME VARCHAR(30) NOT NULL);
CREATE TABLE LIST_TEMPLATE_OBJECT (TEMPLATE_ID VARCHAR(16) NOT NULL, VIRTUAL_OBJECT_ID VARCHAR(30) NOT NULL );
CREATE TABLE LIST_TEMPLATE_FIELD (ID VARCHAR(16) NOT NULL, TEMPLATE_ID VARCHAR(16) NOT NULL, FIELD_NAME VARCHAR(30) NOT NULL, FIELD_POSITION INTEGER );
CREATE TABLE LIST_TEMPLATE_ORDER_BY (ID VARCHAR(16) NOT NULL, TEMPLATE_ID VARCHAR(16) NOT NULL, FIELD_NAME VARCHAR(30) NOT NULL, FIELD_POSITION INTEGER );
CREATE TABLE LIST_TEMPLATE_GROUP_BY (ID VARCHAR(16) NOT NULL, TEMPLATE_ID VARCHAR(16) NOT NULL, FIELD_NAME VARCHAR(30) NOT NULL, FIELD_POSITION INTEGER );
CREATE TABLE LIST_OPERATOR (ID VARCHAR(16) NOT NULL, OPERATOR_NAME VARCHAR(10) NOT NULL );
CREATE TABLE LIST_CLAUSE (ID VARCHAR(16) NOT NULL, TYPE VARCHAR(20) NOT NULL, TEMPLATE_ID VARCHAR(16) NOT NULL, COLUMN_NAME VARCHAR(32) NOT NULL, OPERATOR_ID VARCHAR(16) NOT NULL, PARAM VARCHAR(30) );
CREATE TABLE LIST_CONSTANT_CLAUSE (ID VARCHAR(16) NOT NULL, CONSTANT VARCHAR(128) NOT NULL );
CREATE TABLE LIST_PARAM_CLAUSE (ID VARCHAR(16) NOT NULL );
CREATE TABLE LIST_JOIN_CLAUSE (ID VARCHAR(16) NOT NULL, PARENT_COLUMN VARCHAR(32) NOT NULL, CHILD_COLUMN VARCHAR(32) NOT NULL );
CREATE TABLE LIST_STATIC_DROPDOWN_CLAUSE (ID VARCHAR(16) NOT NULL, OPTIONS VARCHAR(512) NOT NULL );
CREATE TABLE LIST_SELECT_DROPDOWN_CLAUSE (ID VARCHAR(16) NOT NULL, TABLE_NAME VARCHAR(32) NOT NULL, NAME_COLUMN VARCHAR(32) NOT NULL, VALUE_COLUMN VARCHAR(32) NOT NULL );
CREATE TABLE LIST_QUERY_DROPDOWN_CLAUSE (ID VARCHAR(16) NOT NULL, QUERY_ID VARCHAR(16) NOT NULL );
CREATE TABLE LIST_ENUM_DROPDOWN_CLAUSE (ID VARCHAR(16) NOT NULL, CLASS_NAME VARCHAR(200) NOT NULL );
CREATE TABLE LIST_STORED_QUERY (ID VARCHAR(16) NOT NULL, NAME VARCHAR(30) NOT NULL, TEMPLATE_ID VARCHAR(16) NOT NULL, DATASOURCE_NAME VARCHAR(16) NOT NULL );
CREATE TABLE LIST_STORED_QUERY_VALUE (ID VARCHAR(16) NOT NULL, STORED_QUERY_ID VARCHAR(16) NOT NULL, NAME VARCHAR(16) NOT NULL, VALUE VARCHAR(16) NOT NULL );
INSERT INTO LIST_OPERATOR VALUES('1','=');
INSERT INTO LIST_OPERATOR VALUES('2','>');
INSERT INTO LIST_OPERATOR VALUES('3','<');
INSERT INTO LIST_OPERATOR VALUES('4','IN');
INSERT INTO LIST_OPERATOR VALUES('5','NOT IN');
