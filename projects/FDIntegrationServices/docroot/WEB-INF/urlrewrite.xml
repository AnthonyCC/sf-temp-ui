<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">

	<!--

		Configuration file for UrlRewriteFilter http://tuckey.org/urlrewrite/
	-->
<urlrewrite>

	<!--
		disabled because it's conflicting with "/search/query/(.*)" <rule>
		<from>/search/query/(.*)/page/(.*)/max/(.*)</from> <to
		last="true">/v/$1/search.do?searchTerm=$1&amp;page=$2&amp;max=$3</to>
		</rule>
	-->

	<!-- Search Autocomplete -->
	<rule>
		<from>^/v/([0-9]+)/search/autocomplete/(.*)</from>
		<to last="true">/v/$1/search.do?action=autocomplete&amp;searchTerm=$2&amp;ver=$1</to>
	</rule>

	<!-- Search -->
	<rule>
		<from>^/v/([0-9]+)/search/query/(.*)</from>
		<to last="true">/v/$1/search.do?searchTerm=$2&amp;ver=$1</to>
	</rule>

	<!-- Search with payload -->
	<rule>
		<from>^/v/([0-9]+)/search/</from>
		<to last="true">/v/$1/search.do&amp;ver=$1</to>
	</rule>

	<!-- View Product -->
	<rule>
		<from>^/v/([0-9]+)/product/catid/(.*)/id/(.*)</from>
		<to last="true">/v/$1/product.do?productId=$2&amp;categoryId=$2&amp;ver=$1</to>
	</rule>

	<!-- View More Info -->
	<rule>
		<from>^/v/([0-9]+)/product/moreinfo/catid/(.*)/id/(.*)</from>
		<to last="true">/v/$1/product.do?productId=$3&amp;categoryId=$2&amp;action=moreInfo&amp;ver=$1</to>
	</rule>

	<rule>
		<from>^/v/([0-9]+)/product/getprice/catid/(.*)/id/(.*)</from>
		<to last="true">/v/$1/product.do?productId=$3&amp;categoryId=$2&amp;action=getprice&amp;ver=$1</to>
	</rule>

