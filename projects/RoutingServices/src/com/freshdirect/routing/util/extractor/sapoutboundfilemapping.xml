<?xml version="1.0" encoding="ISO-8859-1"?>
<file-format>
    <converter name="char" class="com.blackbear.flatworm.converters.CoreConverters" method="convertChar" return-type="java.lang.String"/>
    <converter name="decimal" class="com.blackbear.flatworm.converters.CoreConverters" method="convertDecimal" return-type="java.lang.Double"/>
    <record name="orderinfo">       
        <record-definition>
            <bean name="iorderinfo" class="com.freshdirect.routing.util.extractor.OrderInfoModel"/>
            <line>
                <record-element length="4" justify="left" beanref="iorderinfo.plant" type="char"/>
                <record-element length="8" justify="left" beanref="iorderinfo.deliveryDate" type="char"/>
                <record-element length="3" justify="left" beanref="iorderinfo.deliveryModel" type="char"/>
                <record-element length="10" justify="left" beanref="iorderinfo.deliveryZone" type="char"/>
                <record-element length="10" justify="left" beanref="iorderinfo.orderNumber" type="char"/>
                <record-element length="6" justify="left" beanref="iorderinfo.deliveryStartTime" type="char"/>
                <record-element length="6" justify="left" beanref="iorderinfo.deliveryEndTime" type="char"/>
                <record-element length="10" justify="left" beanref="iorderinfo.customerNumber" type="char"/>
                <record-element length="40" justify="left" beanref="iorderinfo.customerName" type="char"/>
                <record-element length="40" justify="left" beanref="iorderinfo.alternateReceiver" type="char"/>
                <record-element length="60" justify="left" beanref="iorderinfo.streetAddress1" type="char"/>
                <record-element length="10" justify="left" beanref="iorderinfo.apartmentNumber" type="char"/>
                <record-element length="40" justify="left" beanref="iorderinfo.streetAddress2" type="char"/>
                <record-element length="40" justify="left" beanref="iorderinfo.city" type="char"/>
                <record-element length="3" justify="left" beanref="iorderinfo.state" type="char"/>
                <record-element length="5" justify="left" beanref="iorderinfo.zipCode" type="char"/>                                                
            </line>
        </record-definition>
    </record>
</file-format>