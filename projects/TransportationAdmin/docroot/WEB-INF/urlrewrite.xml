<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">

<urlrewrite>
	<rule>		
		<from>^/v/([0-9]+)/list/eventmetadata/</from>
		<to last="true">/event.jx?action=getEventLogMtda&amp;ver=$1</to>
	</rule>
	<rule>		
		<from>^/v/([0-9]+)/updateeventlogdata/</from>
		<to last="true">/event.jx?action=updateEventLogMtda&amp;ver=$1</to>
	</rule>	
	<rule>
		<from>^/v/([0-9]+)/list/eventlogs/</from>
		<to last="true">/event.jx?action=getEventLogs&amp;ver=$1</to>
	</rule>
	<rule>
		<from>^/v/([0-9]+)/eventlog/add</from>
		<to last="true">/event.jx?action=addEvent&amp;ver=$1</to>
	</rule>	
	<rule>
		<from>^/v/([0-9]+)/eventlog/update</from>
		<to last="true">/event.jx?action=updateEvent&amp;ver=$1</to>
	</rule>
	<rule>
		<from>^/v/([0-9]+)/list/routes/</from>
		<to last="true">/event.jx?action=getRoutes&amp;ver=$1</to>
	</rule>
	
	<rule>
		<from>^/v/([0-9]+)/list/moteventlogs/</from>
		<to last="true">/event.jx?action=getMotEventLogs&amp;ver=$1</to>
	</rule>
	<rule>
		<from>^/v/([0-9]+)/moteventlog/add</from>
		<to last="true">/event.jx?action=addMotEvent&amp;ver=$1</to>
	</rule>
	<rule>
		<from>^/v/([0-9]+)/list/moteventtypes/</from>
		<to last="true">/event.jx?action=getMotEventTypes&amp;ver=$1</to>
	</rule>
	<rule>		
		<from>^/v/([0-9]+)/list/moteventmetadata/</from>
		<to last="true">/event.jx?action=getMotEventLogMtda&amp;ver=$1</to>
	</rule>
	<rule>		
		<from>^/v/([0-9]+)/updatemoteventlogdata/</from>
		<to last="true">/event.jx?action=updateMotEventLogMtda&amp;ver=$1</to>
	</rule>	
	<rule>
		<from>^/v/([0-9]+)/list/addHocRoutes/</from>
		<to last="true">/event.jx?action=getAddHocRoutes&amp;ver=$1</to>
	</rule>	
	<rule>
		<from>^/v/([0-9]+)/verifymotevent/</from>
		<to last="true">/event.jx?action=verifyMot&amp;ver=$1</to>
	</rule>	
	<rule>
		<from>^/v/([0-9]+)/list/shiftlogs/</from>
		<to last="true">/event.jx?action=getShiftLogs&amp;ver=$1</to>
	</rule>
	<rule>
		<from>^/v/([0-9]+)/shifteventlog/add</from>
		<to last="true">/event.jx?action=addShiftEvent&amp;ver=$1</to>
	</rule>
	
	<rule>		
		<from>^/v/([0-9]+)/list/plantcapacity/(.*)</from>
		<to last="true">/dashboard.jx?action=getPlantCapacity&amp;dayOfWeek=$2&amp;ver=$1</to>
	</rule>
	
	<rule>		
		<from>^/v/([0-9]+)/save/plantcapacity/(.*)</from>
		<to last="true">/dashboard.jx?action=savePlantCapacity&amp;dayOfWeek=$2&amp;ver=$1</to>
	</rule>
	
	<rule>		
		<from>^/v/([0-9]+)/list/plantdispatch/</from>
		<to last="true">/dashboard.jx?action=getPlantDispatch&amp;ver=$1</to>
	</rule>
	
	<rule>		
		<from>^/v/([0-9]+)/save/plantdispatch/</from>
		<to last="true">/dashboard.jx?action=savePlantDispatch&amp;ver=$1</to>
	</rule>
	
	<rule>		
		<from>^/v/([0-9]+)/list/orderrate/exception</from>
		<to last="true">/dashboard.jx?action=getOrderRateExceptions&amp;ver=$1</to>
	</rule>
	
	<rule>		
		<from>^/v/([0-9]+)/save/orderrate/exception</from>
		<to last="true">/dashboard.jx?action=saveOrderRateExceptions&amp;ver=$1</to>
	</rule>
</urlrewrite>

