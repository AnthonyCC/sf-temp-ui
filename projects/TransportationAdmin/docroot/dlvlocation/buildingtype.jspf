	}
	else {
	    document.getElementById('house1').readOnly=false;	
		document.getElementById('svcValidate1').disabled=true;
		document.getElementById('svcScrubbedStreet').readOnly=true;
		document.getElementById('svcAddrLine2').readOnly=true;
		document.getElementById('svcCity').readOnly=true;
		document.getElementById('svcState').readOnly=true;
		document.getElementById('svcZip').readOnly=true;
		
		document.getElementById('svcIncludeMon1').disabled=true;
		document.getElementById('svcIncludeTue1').disabled=true;
		document.getElementById('svcIncludeWed1').disabled=true;
		document.getElementById('svcIncludeThu1').disabled=true;
		document.getElementById('svcIncludeFri1').disabled=true;
		document.getElementById('svcIncludeSat1').disabled=true;
		document.getElementById('svcIncludeSun1').disabled=true;

		document.getElementById('svcHoursOpenMon').readOnly=true;
		document.getElementById('svcHoursOpenTue').readOnly=true;
		document.getElementById('svcHoursOpenWed').readOnly=true;
		document.getElementById('svcHoursOpenThu').readOnly=true;
		document.getElementById('svcHoursOpenFri').readOnly=true;
		document.getElementById('svcHoursOpenSat').readOnly=true;
		document.getElementById('svcHoursOpenSun').readOnly=true;

		document.getElementById('svcHoursCloseMon').readOnly=true;
		document.getElementById('svcHoursCloseTue').readOnly=true;
		document.getElementById('svcHoursCloseWed').readOnly=true;
		document.getElementById('svcHoursCloseThu').readOnly=true;
		document.getElementById('svcHoursCloseFri').readOnly=true;
		document.getElementById('svcHoursCloseSat').readOnly=true;
		document.getElementById('svcHoursCloseSun').readOnly=true;

		document.getElementById('svcCommentMon').readOnly=true;
		document.getElementById('svcCommentTue').readOnly=true;
		document.getElementById('svcCommentWed').readOnly=true;
		document.getElementById('svcCommentThu').readOnly=true;
		document.getElementById('svcCommentFri').readOnly=true;
		document.getElementById('svcCommentSat').readOnly=true;
		document.getElementById('svcCommentSun').readOnly=true;
		document.getElementById('svcCommentTue').readOnly=true;		
		
		
		
	}
}

  </script>
   
