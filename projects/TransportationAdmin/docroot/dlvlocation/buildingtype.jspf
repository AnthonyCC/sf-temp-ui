<script type="text/javascript">
function housef(checked) {
    if(checked){
      document.getElementById("doorman1").checked=false;
      document.getElementById("walkup1").checked=false;
      document.getElementById("elevator1").checked=false;
      document.getElementById("svcEnt1").checked=false;
      document.getElementById('handTruckAllowed1').disabled=true;
      document.getElementById('aptDlvAllowed1').disabled=true;
	  document.getElementById('handTruckAllowed2').disabled=true;
      document.getElementById('aptDlvAllowed2').disabled=true;
      document.getElementById('walkUpFloors').readOnly=true;
 	}
	else {
      document.getElementById("doorman1").enabled=true;
      document.getElementById("walkup1").enabled=true;
      document.getElementById("elevator1").enabled=true;
      document.getElementById("svcEnt1").enabled=true;
      document.getElementById('handTruckAllowed1').enabled=true;
      document.getElementById('aptDlvAllowed1').enabled=true;
	  document.getElementById('handTruckAllowed2').enabled=true;
      document.getElementById('aptDlvAllowed2').enabled=true;
  	}
}



function doormanf(checked) {
    if(checked){
     //alert("doorman Checked");
      document.getElementById('house1').checked=false;
      document.getElementById('handTruckAllowed1').disabled=false;
      document.getElementById('aptDlvAllowed1').disabled=false;
      document.getElementById('handTruckAllowed2').disabled=false;
      document.getElementById('aptDlvAllowed2').disabled=false;
	}
	else {
	  //  alert("doorman Unchecked");
	  document.getElementById('house1').enabled=true;
	  document.getElementById('handTruckAllowed1').disabled=true;
      document.getElementById('aptDlvAllowed1').disabled=true;
	  document.getElementById('handTruckAllowed2').disabled=true;
      document.getElementById('aptDlvAllowed2').disabled=true;
  	}
}

function walkupf(checked) {
    if(checked){
      //alert("walkup checked");
      document.getElementById('house1').checked=false;
      document.getElementById('elevator1').checked=false;
      document.getElementById('walkUpFloors').readOnly=false;
 
	}
	else {
      document.getElementById('house1').enabled=true;
      document.getElementById('elevator1').enabled=true;
      document.getElementById('walkUpFloors').readOnly=true;
 	}
}

function elevatorf(checked) {
    if(checked){
      //alert("elev checked");
      document.getElementById('house1').checked=false;
      document.getElementById('walkup1').checked=false;
      document.getElementById('walkUpFloors').readOnly=true;
 	}
	else {
      document.getElementById('house1').enabled=true;
      document.getElementById('walkup1').enabled=true;
 	}
}

function svcEntf(checked) {
    if(checked){
        document.getElementById('house1').checked=false;   
		document.getElementById('svcValidate1').disabled=false;
		document.getElementById('svcScrubbedStreet').readOnly=false;
		document.getElementById('svcAddrLine2').readOnly=false;
		document.getElementById('svcCity').readOnly=false;
		document.getElementById('svcState').readOnly=false;
		document.getElementById('svcZip').readOnly=false;
		if(svcScrubbedStreet.value == ""){  
			document.getElementById('svcScrubbedStreet').value=document.getElementById('building.srubbedStreet').value;
			document.getElementById('svcCity').value=document.getElementById('building.city').value;
			document.getElementById('svcState').value=document.getElementById('building.state').value;
			document.getElementById('svcZip').value=document.getElementById('building.zip').value;
		}
		document.getElementById('svcIncludeMon1').disabled=false;
		document.getElementById('svcIncludeTue1').disabled=false;
