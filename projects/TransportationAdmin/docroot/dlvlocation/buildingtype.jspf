		document.getElementById('svcHoursOpenTue').readOnly=false;
		document.getElementById('svcHoursOpenWed').readOnly=false;
		document.getElementById('svcHoursOpenThu').readOnly=false;
		document.getElementById('svcHoursOpenFri').readOnly=false;
		document.getElementById('svcHoursOpenSat').readOnly=false;
		document.getElementById('svcHoursOpenSun').readOnly=false;

		document.getElementById('svcHoursCloseMon').readOnly=false;
		document.getElementById('svcHoursCloseTue').readOnly=false;
		document.getElementById('svcHoursCloseWed').readOnly=false;
		document.getElementById('svcHoursCloseThu').readOnly=false;
		document.getElementById('svcHoursCloseFri').readOnly=false;
		document.getElementById('svcHoursCloseSat').readOnly=false;
		document.getElementById('svcHoursCloseSun').readOnly=false;

		document.getElementById('svcCommentMon').readOnly=false;
		document.getElementById('svcCommentTue').readOnly=false;
		document.getElementById('svcCommentWed').readOnly=false;
		document.getElementById('svcCommentThu').readOnly=false;
		document.getElementById('svcCommentFri').readOnly=false;
