<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.freshdirect.delivery.model" schema="dlv">

    <class name="DlvRegionDataModel" table="REGION_DATA" lazy="false">
        <id name="id" column="ID">
            <generator class="sequence">
            	<param name="sequence">SYSTEM_SEQ</param>
            </generator>
        </id>

        <property name="startDate" column="START_DATE" type="date" not-null="true"/>
        <property name="deliveryCharges" column="DELIVERY_CHARGES"/>
        
         <bag name="zones" cascade="all,delete-orphan" lazy="false">
            <key column="REGION_DATA_ID" not-null="true"/>
            <one-to-many class="DlvZoneModel"/>
        </bag>

    </class>

</hibernate-mapping>