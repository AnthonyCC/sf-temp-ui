<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB//EN' 'http://www.bea.com/servers/wls510/dtd/weblogic-ejb-jar.dtd'>

<!-- Created by knadeem on August 1, 2001, 8:54 PM -->

<weblogic-ejb-jar>

	<weblogic-enterprise-bean>
        <ejb-name>SessionAuditor</ejb-name>
        <caching-descriptor>
	        <max-beans-in-free-pool>50</max-beans-in-free-pool>
	        <max-beans-in-cache>10</max-beans-in-cache>
	        <idle-timeout-seconds>60</idle-timeout-seconds>
        </caching-descriptor>
        <persistence-descriptor>
	        <delay-updates-until-end-of-tx>false</delay-updates-until-end-of-tx>
        </persistence-descriptor>
		<reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/dbpool</res-ref-name>
                <jndi-name>fddatasource</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>freshdirect.SessionAuditor</jndi-name>
    </weblogic-enterprise-bean>

	<weblogic-enterprise-bean>
        <ejb-name>DlvManager</ejb-name>
        <caching-descriptor>
	        <max-beans-in-free-pool>50</max-beans-in-free-pool>
	        <max-beans-in-cache>10</max-beans-in-cache>
	        <idle-timeout-seconds>60</idle-timeout-seconds>
        </caching-descriptor>
        <persistence-descriptor>
	        <delay-updates-until-end-of-tx>true</delay-updates-until-end-of-tx>
        </persistence-descriptor>
		<reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/dbpool</res-ref-name>
                <jndi-name>fddatasource</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>freshdirect.delivery.DeliveryManager</jndi-name>
    </weblogic-enterprise-bean>
	
	
	<weblogic-enterprise-bean>
        <ejb-name>DlvRestrictionManager</ejb-name>
        <caching-descriptor>
	        <max-beans-in-free-pool>50</max-beans-in-free-pool>
	        <max-beans-in-cache>10</max-beans-in-cache>
	        <idle-timeout-seconds>60</idle-timeout-seconds>
        </caching-descriptor>
        <persistence-descriptor>
	        <delay-updates-until-end-of-tx>true</delay-updates-until-end-of-tx>
        </persistence-descriptor>
		<reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/dbpool</res-ref-name>
                <jndi-name>fddatasource</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>freshdirect.delivery.DeliveryRestrictionManager</jndi-name>
    </weblogic-enterprise-bean>
	
	<weblogic-enterprise-bean>
        <ejb-name>DlvAdminManager</ejb-name>
        <caching-descriptor>
	        <max-beans-in-free-pool>50</max-beans-in-free-pool>
	        <max-beans-in-cache>10</max-beans-in-cache>
	        <idle-timeout-seconds>60</idle-timeout-seconds>
        </caching-descriptor>
        <persistence-descriptor>
	        <delay-updates-until-end-of-tx>true</delay-updates-until-end-of-tx>
        </persistence-descriptor>
		<reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/dbpool</res-ref-name>
                <jndi-name>fddatasource</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>freshdirect.delivery.AdminManager</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>DlvMapper</ejb-name>
        <caching-descriptor>
	        <max-beans-in-free-pool>50</max-beans-in-free-pool>
	        <max-beans-in-cache>10</max-beans-in-cache>
	        <idle-timeout-seconds>60</idle-timeout-seconds>
        </caching-descriptor>
        <persistence-descriptor>
	        <delay-updates-until-end-of-tx>true</delay-updates-until-end-of-tx>
        </persistence-descriptor>
		<reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/dbpool</res-ref-name>
                <jndi-name>fddatasource</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>freshdirect.delivery.Mapper</jndi-name>
    </weblogic-enterprise-bean>
    
	<weblogic-enterprise-bean>
        <ejb-name>DlvTemplateManager</ejb-name>
        <caching-descriptor>
	        <max-beans-in-free-pool>50</max-beans-in-free-pool>
	        <max-beans-in-cache>10</max-beans-in-cache>
	        <idle-timeout-seconds>60</idle-timeout-seconds>
        </caching-descriptor>
        <persistence-descriptor>
	        <delay-updates-until-end-of-tx>true</delay-updates-until-end-of-tx>
        </persistence-descriptor>
		<reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/dbpool</res-ref-name>
