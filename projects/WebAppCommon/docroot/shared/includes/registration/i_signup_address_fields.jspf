		<label for="<%= EnumUserInfoName.DLV_CITY.getCode() %>">*&nbsp;City</label>
		<fd:ErrorHandler result='<%=result%>' name='<%=EnumUserInfoName.DLV_CITY.getCode()%>'></span></fd:ErrorHandler>
