		<fd:ErrorHandler result='<%=result%>' name='<%= EnumUserInfoName.DLV_ZIPCODE.getCode() %>' id='errorMsg'><span id="zipcode_signup_error" class="errortext"><%=errorMsg%></span></fd:ErrorHandler></td>
