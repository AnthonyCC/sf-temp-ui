{namespace browse}

/**
 * @param? carousel1 carousel1
 * @param? carousel2 carousel2
 * @param? carouselPosition position
 * @param? carouselRatio ratio
 */
{template .bottomCarousels autoescape="false"}
  {if $carouselPosition == 'BOTTOM'}
    {call browse.classedCarousels}
      {param carousel1:$carousel1 /}
      {param carousel2:$carousel2 /}
      {param cssclass:$carouselRatio?$carouselRatio:'bottom' /}
    {/call}
  {/if}
{/template}


