{namespace common}

/**
 * @param item
 */
{template .iconCarouselItem}
<li class="portrait-item">
  <div class="icon-image">
    <a href="{$item.iconLink}">
      <img src="{$item.iconImage}" alt="icon">
    </a>
  </div>
  <a href="$item.iconLink">{if $item.iconLinkText}{$item.iconLinkText}{/if}</a>
</li>
{/template}
