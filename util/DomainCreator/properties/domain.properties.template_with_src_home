#set weblogic home dir
weblogic.home=/home/tamas/local/bea/wlserver_10.3

#domain dir will be created in this dir
domain.home=/home/tamas/FreshDirect

#name of the domain
domain.name=domain

# 7001 is the default in the code and configs
server.port=7001

#this is required
domain.home.ignoreSourceRepositoryCheck

#the root of the FD source
src.home=/home/tamas/src/FreshDirect/01