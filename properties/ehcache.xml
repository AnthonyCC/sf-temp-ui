<ehcache>

    <defaultCache
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="3600"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </defaultCache>

    <cache name="pastOrdersCache"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="3600"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

	<cache name="pastOrdersSearchCache"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="3600"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

	<cache name="yourListsCache"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="3600"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

	<cache name="fdListsCache"
           maxEntriesLocalHeap="100"
           eternal="false"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>
    
</ehcache>
